<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/batteryFragment"
        android:name="com.softcleean.fastcleaner.functions.battery.BatteryFragment"
        android:label="fragment_battery"
        tools:layout="@layout/fragment_battery" >
        <action
            android:id="@+id/action_batteryFragment_to_batteryOptimizingFragment"
            app:destination="@id/batteryOptimizingFragment" />
    </fragment>
    <fragment
        android:id="@+id/batteryOptimizingFragment"
        android:name="com.softcleean.fastcleaner.functions.battery.BatteryOptimizingFragment"
        android:label="fragment_battery_optimizing"
        tools:layout="@layout/fragment_battery_optimizing" >
        <action
            android:id="@+id/action_batteryOptimizingFragment_to_batteryResultFragment"
            app:destination="@id/batteryResultFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="false"  />
    </fragment>
    <fragment
        android:id="@+id/boostFragment"
        android:name="com.softcleean.fastcleaner.functions.boost.BoostFragment"
        android:label="fragment_boost"
        tools:layout="@layout/fragment_boost" >
        <action
            android:id="@+id/action_boostFragment_to_boostOptimizingFragment"
            app:destination="@id/boostOptimizingFragment" />
    </fragment>
    <fragment
        android:id="@+id/boostOptimizingFragment"
        android:name="com.softcleean.fastcleaner.functions.boost.BoostOptimizingFragment"
        android:label="BoostOptimizingFragment" >
        <action
            android:id="@+id/action_boostOptimizingFragment_to_boostResultFragment"
            app:destination="@id/boostResultFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="false"  />
    </fragment>
    <fragment
        android:id="@+id/coolingFragment"
        android:name="com.softcleean.fastcleaner.functions.cooling.CoolingFragment"
        android:label="CoolingFragment" >
        <action
            android:id="@+id/action_coolingFragment_to_coolingOptimizingFragment"
            app:destination="@id/coolingOptimizingFragment" />
    </fragment>
    <fragment
        android:id="@+id/coolingOptimizingFragment"
        android:name="com.softcleean.fastcleaner.functions.cooling.CoolingOptimizingFragment"
        android:label="CoolingOptimizingFragment" >
        <action
            android:id="@+id/action_coolingOptimizingFragment_to_coolingResultFragment"
            app:destination="@id/coolingResultFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/cleanFragment"
        android:name="com.softcleean.fastcleaner.functions.clean.CleanFragment"
        android:label="CleanFragment" >
        <action
            android:id="@+id/action_cleanFragment_to_cleanOptimizingFragment"
            app:destination="@id/cleanOptimizingFragment" />
    </fragment>
    <fragment
        android:id="@+id/cleanOptimizingFragment"
        android:name="com.softcleean.fastcleaner.functions.clean.CleanOptimizingFragment"
        android:label="fragment_clean_optimizing"
        tools:layout="@layout/fragment_clean_optimizing" >
        <action
            android:id="@+id/action_cleanOptimizingFragment_to_cleanResultFragment"
            app:destination="@id/cleanResultFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/scanningFragment"
        android:name="com.softcleean.fastcleaner.functions.scanning.ScanningFragment"
        android:label="ScanningFragment" >
        <action
            android:id="@+id/action_scanningFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/scanningFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <action
        android:id="@+id/action_to_homeFragment"
        app:destination="@id/homeFragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.softcleean.fastcleaner.functions.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
    </fragment>
    <action
        android:id="@+id/action_to_batteryFragment"
        app:destination="@id/batteryFragment" />
    <action
        android:id="@+id/action_to_boostFragment"
        app:destination="@id/boostFragment" />
    <action
        android:id="@+id/action_to_coolingFragment"
        app:destination="@id/coolingFragment" />
    <action
        android:id="@+id/action_to_cleanFragment"
        app:destination="@id/cleanFragment" />
    <fragment
        android:id="@+id/cleanResultFragment"
        android:name="com.softcleean.fastcleaner.functions.clean.CleanResultFragment"
        android:label="CleanResultFragment" />
    <fragment
        android:id="@+id/coolingResultFragment"
        android:name="com.softcleean.fastcleaner.functions.cooling.CoolingResultFragment"
        android:label="CoolingResultFragment" />
    <fragment
        android:id="@+id/boostResultFragment"
        android:name="com.softcleean.fastcleaner.functions.boost.BoostResultFragment"
        android:label="BoostResultFragment" />
    <fragment
        android:id="@+id/batteryResultFragment"
        android:name="com.softcleean.fastcleaner.functions.battery.BatteryResultFragment"
        android:label="fragment_battery_result"
        tools:layout="@layout/fragment_battery_result" />
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.softcleean.fastcleaner.functions.splash.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splashFragment_to_scanningFragment"
            app:destination="@id/scanningFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>
<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/scanningFragment">

    <fragment
        android:id="@+id/batteryFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.battery.BatteryFragment"
        android:label="fragment_battery"
        tools:layout="@layout/fragment_battery" >
        <action
            android:id="@+id/action_batteryFragment_to_batteryOptimizingFragment"
            app:destination="@id/batteryOptimizingFragment" />
    </fragment>
    <fragment
        android:id="@+id/batteryOptimizingFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.battery.BatteryOptimizingFragment"
        android:label="fragment_battery_optimizing"
        tools:layout="@layout/fragment_battery_optimizing" />
    <fragment
        android:id="@+id/boostFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.boost.BoostFragment"
        android:label="fragment_boost"
        tools:layout="@layout/fragment_boost" >
        <action
            android:id="@+id/action_boostFragment_to_boostOptimizingFragment"
            app:destination="@id/boostOptimizingFragment" />
    </fragment>
    <fragment
        android:id="@+id/boostOptimizingFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.boost.BoostOptimizingFragment"
        android:label="BoostOptimizingFragment" />
    <fragment
        android:id="@+id/coolingFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.cooling.CoolingFragment"
        android:label="CoolingFragment" >
        <action
            android:id="@+id/action_coolingFragment_to_coolingOptimizingFragment"
            app:destination="@id/coolingOptimizingFragment" />
    </fragment>
    <fragment
        android:id="@+id/coolingOptimizingFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.cooling.CoolingOptimizingFragment"
        android:label="CoolingOptimizingFragment" />
    <fragment
        android:id="@+id/cleanFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.clean.CleanFragment"
        android:label="CleanFragment" >
        <action
            android:id="@+id/action_cleanFragment_to_cleanOptimizingFragment"
            app:destination="@id/cleanOptimizingFragment" />
    </fragment>
    <fragment
        android:id="@+id/cleanOptimizingFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.clean.CleanOptimizingFragment"
        android:label="fragment_clean_optimizing"
        tools:layout="@layout/fragment_clean_optimizing" />
    <fragment
        android:id="@+id/scanningFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.scanning.ScanningFragment"
        android:label="ScanningFragment" >
        <action
            android:id="@+id/action_scanningFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.entertainment.event.ssearch.ar155.functions.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_batteryFragment"
            app:destination="@id/batteryFragment" />
        <action
            android:id="@+id/action_homeFragment_to_boostFragment"
            app:destination="@id/boostFragment" />
        <action
            android:id="@+id/action_homeFragment_to_coolingFragment"
            app:destination="@id/coolingFragment" />
        <action
            android:id="@+id/action_homeFragment_to_cleanFragment"
            app:destination="@id/cleanFragment" />
    </fragment>
</navigation>